{
  "name": "yt-dlp-downloader",
  "version": "0.0.1",
  "description": "Cross-platform video downloader with yt-dlp",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx src/",
    "dev": "electron-forge start -- --dev",
    "build": "electron-forge make",
    "download-binaries": "cross-env HTTPS_PROXY=http://127.0.0.1:7890 ts-node scripts/download-binaries.ts",
    "download-binaries:no-proxy": "ts-node scripts/download-binaries.ts",
    "init": "ts-node scripts/download-binaries.ts",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "video-downloader",
    "yt-dlp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "config": {
    "forge": "./forge.config.ts"
  },
  "dependencies": {
    "tree-kill": "^1.0.0",
    "@electron-forge/cli": "^7.x.x",
    "@types/node": "^20.x.x",
    "@types/react": "^18.x.x",
    "@types/react-dom": "^18.x.x",
    "react": "^18.x.x",
    "react-dom": "^18.x.x"
  },
  "devDependencies": {
    "@electron-forge/maker-deb": "^7.x.x",
    "@electron-forge/maker-rpm": "^7.x.x",
    "@electron-forge/maker-squirrel": "^7.x.x",
    "@electron-forge/maker-zip": "^7.x.x",
    "@electron-forge/plugin-webpack": "^7.6.0",
    "@types/extract-zip": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^6.x.x",
    "@typescript-eslint/parser": "^6.x.x",
    "autoprefixer": "^10.x.x",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "electron": "^29.4.6",
    "eslint": "^8.x.x",
    "eslint-plugin-react": "^7.x.x",
    "eslint-plugin-react-hooks": "^4.x.x",
    "extract-zip": "^2.0.1",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.x.x",
    "ts-loader": "^9.x.x",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "node-loader": "^2.0.0"
  },
  "build": {
    "extraResources": [
      {
        "from": "lib",
        "to": "lib",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
